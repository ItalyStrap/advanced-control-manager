{
	"name": "ItalyStrap/advanced-control-manager",
	"description": "Make your web site more powerful",
	"type": "wordpress-plugin",
	"license": "GPL 2.0",
    "minimum-stability": "dev",
	"require": {
		"php": ">=5.3",
		"mobiledetect/mobiledetectlib": "dev-master",
		"cmb2/cmb2": "dev-master",
		"overclokk/minimum-requirements": "^1.0",
		"rdlowrey/auryn": "^1.4",
		"erusev/parsedown": "^1.6",
		"inpsyde/menu-cache": "^1.0",
        "italystrap/edd": "dev-master",
        "italystrap/breadcrumbs": "^1.0",
        "italystrap/fields": "^1.0",
        "italystrap/config": "^1.0",
		"italystrap/helpers": "~1.0",
        "italystrap/debug": "^1.0"
	},
	"require-dev": {
		"lucatume/wp-browser": "~2.2",
		"leafo/scssphp": "^0.6.5"
	},
	"autoload": {
		"classmap": [
			"admin",
			"deprecated"
		],
		"psr-4": {
			"ItalyStrap\\": "src/"
		}
	},
	"autoload-dev": {

	},
	"extra": {
		"installer-paths": {
			"vendor/{$name}/": ["cmb2/cmb2"]
		}
	},
	"authors": [
		{
			"name": "overclokk",
			"email": "info@overclokk.net"
		}
	]
}
