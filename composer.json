{
    "name": "overclokk/italystrap-extended",
    "description": "Make your site more powerful",
    "type": "wordpress-plugin",
    "license": "GPL 2.0",
	"require": {
		"php": "^5.3 || ^5.4 || ^5.5 || ^5.6 || ^7.0",
		"mobiledetect/mobiledetectlib": "dev-master",
		"webdevstudios/cmb2": "dev-master",
		"overclokk/minimum-requirements": "dev-master",
		"rdlowrey/auryn": "^1.4",
        "erusev/parsedown": "^1.6",
        "leafo/scssphp": "^0.6.5",
        "inpsyde/menu-cache": "^1.0"
	},
	"require-dev": {
		"lucatume/wp-browser": "^1.9",
		"raveren/kint": "~1.0",
		"filp/whoops": "~2.1.0"
	},
	"autoload": {
		"classmap": [
			"admin",
			"deprecated"
		],
		"psr-4": {
			"ItalyStrap\\Settings\\": "src/settings/src/",
			"ItalyStrap\\Fields\\": "src/fields/src/",
			"ItalyStrap\\Import_Export\\": "src/import_export/src/",
			"ItalyStrap\\Core\\": "core/",
			"ItalyStrap\\Widget\\": "widget/",
			"ItalyStrap\\Shortcode\\": "shortcode/",
			"ItalyStrap\\Migrations\\": "src/migrations/src/",
			"ItalyStrap\\Event\\": "src/event/src/",
			"ItalyStrap\\Debug\\": "src/debug/src/",
			"ItalyStrap\\Config\\": "src/config/src/"
		}
	},
	"autoload-dev": {

	},
	"extra": {
		"installer-paths": {
			"vendor/{$name}/": ["webdevstudios/cmb2"]
		}
	},
    "authors": [
        {
            "name": "overclokk",
            "email": "info@overclokk.net"
        }
    ]
}
